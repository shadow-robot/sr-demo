<package>
  <description brief="sr_gui_demo1">

     sr_gui_demo1 is a rosgui plugin for launching the shadow robot demo.

  </description>
  <author>Gianluigi Petroni</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/sr_gui_demo1</url>
  <depend package="rospy"/>
  <depend package="sr_hand"/>
  <depend package="actionlib"/>
  <depend package="roslib"/>
  <depend package="roscpp"/>
  <depend package="std_msgs"/>
  <depend package="actionlib_msgs"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_py"/>

  <export>
    <rqt_gui plugin="${prefix}/sr_gui_demo1.xml"/>
  </export>

</package>


